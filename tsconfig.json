{
	"extends": "./node_modules/@tsconfig/node16/tsconfig.json",
	"compilerOptions": {
		"lib": ["ESNext"],
		"target": "ESNext",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": true,
		"sourceMap": true,
		"outDir": "js-build",
		"baseUrl": "src",
		"paths": {}
	},
	"include": ["src/**/*.ts", "serverless.ts"],
	"exclude": [
		"node_modules/**/*",
		".serverless/**/*",
		".webpack/**/*",
		"_warmup/**/*",
		".vscode/**/*"
	]
}